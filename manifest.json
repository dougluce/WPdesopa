{
  "name": "WPDeSopa",
  "version": "1.0",
  "description": "Make Wikipedia visible.  By doug@desopa.con.com.",
  "content_security_policy": "default-src 'none'; script-src 'self'",
  "content_scripts": [
     {
       "matches": ["http://*.wikipedia.org/*"],
       "js": ["WPdesopa.js"]
     }
  ]
}
